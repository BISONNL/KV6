			<xs:element name="vehiclenumber" type="tmi8:vehicleidnumberType"/>
			<xs:element name="distancesincelastuserstop" type="tmi8:distancesincelastuserstopType" minOccurs="0"/>
			<xs:element name="rd-x" type="tmi8:rdType"/>
